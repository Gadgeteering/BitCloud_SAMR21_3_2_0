     56          /**************************************************************************//**
     57          \brief Initializes diagnostics cluster
     58          ******************************************************************************/
     59          void diagnosticsClusterInit(void)
     60          {
     61            msDiagnosticsClusterServerAttributes.averageMACRetryPerAPSMessageSent.value = 0;
     62            msDiagnosticsClusterServerAttributes.lastMessageLQI.value = 0;
     63            msDiagnosticsClusterServerAttributes.lastMessageRSSI.value = 0;
     64            SYS_SubscribeToEvent(BC_EVENT_APS_DATA_INDICATION, &msEventReceiver);
     65            SYS_SubscribeToEvent(BC_EVENT_NWK_DATA_CONFIRM, &msEventReceiver);
     66          }
     67          
     68          static void msDiagnosticsEventListener(SYS_EventId_t eventId, SYS_EventData_t data)
     69          {
     70            switch(eventId)
     71            {
     72              case BC_EVENT_APS_DATA_INDICATION:
     73                if(NULL != (APS_DataQualityIndicator_t *)data)
     74                {
     75                  APS_DataQualityIndicator_t *apsDataQualityIndicator = (APS_DataQualityIndicator_t *)data;
     76                  msDiagnosticsClusterServerAttributes.lastMessageLQI.value = apsDataQualityIndicator->linkQuality;
     77                  msDiagnosticsClusterServerAttributes.lastMessageRSSI.value = apsDataQualityIndicator->rssi;
     78                }
     79                break;
     80          
     81              case BC_EVENT_NWK_DATA_CONFIRM:
     82                if(NULL != (uint16_t*)data)
     83                {
     84                  static bool firstPacket = 1;
     85                  if(firstPacket)
     86                  {
     87                    msDiagnosticsClusterServerAttributes.averageMACRetryPerAPSMessageSent.value
     88                      = *(uint16_t*)data;
     89                    firstPacket = 0;
     90                  }
     91                  else
     92                  {
     93                    msDiagnosticsClusterServerAttributes.averageMACRetryPerAPSMessageSent.value
     94                      = (msDiagnosticsClusterServerAttributes.averageMACRetryPerAPSMessageSent.value 
     95                        + *(uint16_t*)data) / 2;
     96                  }
     97                }
     98                break;
     99              default:
    100                break;
    101            }
    102          }
