     51          /**************************************************************************//**
     52          \brief Register new generator of stat data
     53          
     54          \param[in] gen - generator
     55          ******************************************************************************/
     56          void SYS_RegisterStatGenerator(SYS_StatGenerator_t *gen)
     57          {
     58            putQueueElem(&statGenerators, gen);
     59          }
     60          
     61          /**************************************************************************//**
     62          \brief Collect stats from registered generators into the supplied buffer
     63          
     64          \param[out] buf - buffer to place stat report
     65          \param[in] maxSize - size limit of buffer
     66          \return number of actually placed bytes
     67          ******************************************************************************/
     68          uint8_t SYS_GenerateStatReport(uint8_t *buf, uint8_t maxSize)
     69          {
     70            const SYS_StatGenerator_t *gen;
     71            uint8_t *p = buf;
     72          
     73            // Assume generators are polite and won't make maxSize negative
     74            for (gen = getQueueElem(&statGenerators); gen && maxSize; gen = getNextQueueElem(gen))
     75            {
     76              uint8_t size = gen->gen(p, maxSize);
     77              maxSize -= size;
     78              p += size;
     79            }
     80            return p - buf;
     81          }
