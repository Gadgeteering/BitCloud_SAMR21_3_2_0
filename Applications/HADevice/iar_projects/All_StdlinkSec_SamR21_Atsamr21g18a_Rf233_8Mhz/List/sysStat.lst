     86          // Preincluded generator for reporting stack usage. Only classic AVRs are supported for now
     87          
     88          /**************************************************************************//**
     89          \brief Measure stack usage by searching for first overwritten byte of
     90                 default fill pattern
     91          
     92          \param[in] start - start of section
     93          \param[in] end - end of section (address of byte after the last one)
     94          \return number of used bytes
     95          ******************************************************************************/
     96          static inline uint16_t probeStack(const volatile uint8_t *start, const volatile uint8_t *end)
     97          {
     98            while (start < end && *start == 0xCD)
     99              start++;
    100            return end - start;
    101          }
    102          
    103          /**************************************************************************//**
    104          \brief Stat generator for reporting stacks usage
    105          
    106          \param[out] buf - output buffer
    107          \param[in] maxSize - buffer size limit
    108          \return number of bytes actually written
    109          ******************************************************************************/
    110          #if defined(__ICCAVR__)
    111          static uint8_t measureStack(uint8_t *buf, uint8_t maxSize)
    112          {
    113            #pragma section = "CSTACK"
    114            #pragma section = "RSTACK"
    115            uint16_t used;
    116          
    117            if (maxSize < 2)
    118              return 0;
    119          
    120            used = probeStack(__segment_begin("CSTACK"), __segment_end("CSTACK"));
    121            memcpy(buf, &used, 2);
    122          
    123            if (maxSize < 4)
    124              return 2;
    125          
    126            used = probeStack(__segment_begin("RSTACK"), __segment_end("RSTACK"));
    127            memcpy(buf + 2, &used, 2);
    128          
    129            return 4;
    130          }
    131          #elif defined(__GNUC__)
    132          static uint8_t measureStack(uint8_t *buf, uint8_t maxSize)
    133          {
    134            extern uint16_t __stack_start;
    135            extern uint16_t __stack;
    136            uint16_t used;
    137          
    138            if (maxSize < 2)
    139              return 0;
    140          
    141            used = probeStack((volatile uint8_t *)&__stack_start, ((volatile uint8_t *)&__stack) + 1);
    142            memcpy(buf, &used, 2);
    143          
    144            return 2;
    145          }
    146          #endif
    147          #endif
