     39          void temperatureMeasurementClusterInit(void)
     40          {
     41            tsTemperatureMeasurementClusterServerAttributes.measuredValue.value = APP_TEMPERATURE_MEASUREMENT_MEASURED_VALUE_ATTRIBUTE_VALUE;
     42            tsTemperatureMeasurementClusterServerAttributes.minMeasuredValue.value = APP_TEMPERATURE_MEASUREMENT_MIN_MEASURED_VALUE_ATTRIBUTE_VALUE;
     43            tsTemperatureMeasurementClusterServerAttributes.maxMeasuredValue.value = APP_TEMPERATURE_MEASUREMENT_MAX_MEASURED_VALUE_ATTRIBUTE_VALUE;
     44            tsTemperatureMeasurementClusterServerAttributes.tolerance.value = APP_TEMPERATURE_MEASUREMENT_TOLERANCE_ATTRIBUTE_VALUE;//to be updated
     45          }
     46          
     47          /**************************************************************************//**
     48          \brief Update the measured Value
     49          ******************************************************************************/
     50          void tempeartureMeasurementUpdateMeasuredValue(void)
     51          {
     52            int16_t measuredValue;
     53          
     54            measuredValue = tsTemperatureMeasurementClusterServerAttributes.measuredValue.value - APP_TEMPERATURE_MEASUREMENT_MEASURED_VALUE_PERIODIC_CHANGE;
     55          
     56            if (measuredValue < tsTemperatureMeasurementClusterServerAttributes.minMeasuredValue.value)
     57              tsTemperatureMeasurementClusterServerAttributes.measuredValue.value =  tsTemperatureMeasurementClusterServerAttributes.maxMeasuredValue.value;
     58            else
     59              tsTemperatureMeasurementClusterServerAttributes.measuredValue.value = measuredValue;
     60          
     61            ZCL_ReportOnChangeIfNeeded(&tsTemperatureMeasurementClusterServerAttributes.measuredValue);
     62          }
