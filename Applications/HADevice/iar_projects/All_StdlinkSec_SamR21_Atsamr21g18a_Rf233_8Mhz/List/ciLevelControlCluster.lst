    210          \param[in] addr - short address of destination node;
    211          \param[in] ep   - destination endpoint;
    212          \param[in] attrId - attribute id;
    213          \param[in] attrType - attribute type
    214          \param[in] min  - the minimum reporting interval;
    215          \param[in] max  - the maximum reporting interval
    216          ******************************************************************************/
    217          void levelControlConfigureReporting(APS_AddrMode_t mode, ShortAddr_t addr, Endpoint_t ep,
    218            ZCL_AttributeId_t attrId, uint8_t attrType, ZCL_ReportTime_t min, ZCL_ReportTime_t max)
    219          {
    220            ZCL_Request_t *req;
    221            ZCL_NextElement_t element;
    222            ZCL_ConfigureReportingReq_t configureReportingReq;
    223          
    224            if (!(req = getFreeCommand()))
    225              return;
    226          
    227            configureReportingReq.direction            = ZCL_FRAME_CONTROL_DIRECTION_CLIENT_TO_SERVER;
    228            configureReportingReq.attributeId          = attrId;
    229            configureReportingReq.attributeType        = attrType;
    230            configureReportingReq.minReportingInterval = min;
    231            configureReportingReq.maxReportingInterval = max;
    232          
    233            element.payloadLength = 0;
    234            element.payload = req->requestPayload;
    235            element.id = ZCL_CONFIGURE_REPORTING_COMMAND_ID;
    236            element.content = &configureReportingReq;
    237            ZCL_PutNextElement(&element);
    238          
    239            fillCommandRequest(req, ZCL_CONFIGURE_REPORTING_COMMAND_ID, element.payloadLength);
    240            fillDstAddressing(&req->dstAddressing, mode, addr, ep, LEVEL_CONTROL_CLUSTER_ID);
    241            req->ZCL_Notify = ZCL_ConfigureReportingResp;
    242          
    243            commandManagerSendAttribute(req);
    244          }
