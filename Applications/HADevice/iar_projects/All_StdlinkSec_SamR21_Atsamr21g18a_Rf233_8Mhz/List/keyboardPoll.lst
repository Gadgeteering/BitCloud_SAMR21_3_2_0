     31          /**************************************************************************//**
     32          \brief Poll if any key is pressed and call processing routint if any
     33          ******************************************************************************/
     34          void pollKeyboard(void)
     35          {
     36            int ch;
     37          
     38            if (0 != _kbhit())
     39            {
     40              ch = _getch();
     41              putch(ch);
     42              consoleRx(ch);
     43              if ('\r' == ch)
     44              {
     45                putch('\n');
     46                consoleRx('\n');
     47              }
     48            }
     49          }
