     39          /**************************************************************************//**
     40          \brief Calculates maximum Lumen value for  X/Y color.
     41          
     42          \param[in]
     43              valX - X value of the color
     44          \param[in]
     45              valY - Y value of the color
     46          
     47          \param[out]
     48              calcMaxL - Pointer to the calculated maximum Lumen value for X/Y color
     49          ******************************************************************************/
     50          void maxL_FromXY(unsigned int valX, unsigned int valY, int *calcMaxL)
     51          {
     52            float x,y,maxl;
     53            unsigned int xi, yi;
     54          
     55            xi = valX / 6553;
     56            yi = valY / 6553;
     57          
     58            x = (float)valX/65535.0f - (float)xi/10.0f;
     59            y = (float)valY/65535.0f - (float)yi/10.0f;
     60          
     61            maxl = (float)s_maxL[9-yi][xi]*(1-x)*(1-y) +
     62                   (float)s_maxL[9-yi][xi+1]*x*(1-y) +
     63                   (float)s_maxL[9-(yi+1)][xi]*(1-x)*y +
     64                   (float)s_maxL[9-(yi+1)][xi+1]*x*y;
     65          
     66            *calcMaxL = (int) maxl;
     67          }
