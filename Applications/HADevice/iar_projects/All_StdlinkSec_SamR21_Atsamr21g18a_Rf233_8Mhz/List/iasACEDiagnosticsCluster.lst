     56          /**************************************************************************//**
     57          \brief Initializes diagnostics cluster
     58          ******************************************************************************/
     59          void diagnosticsClusterInit(void)
     60          {
     61            iasACEDiagnosticsClusterServerAttributes.averageMACRetryPerAPSMessageSent.value = 0;
     62            iasACEDiagnosticsClusterServerAttributes.lastMessageLQI.value = 0;
     63            iasACEDiagnosticsClusterServerAttributes.lastMessageRSSI.value = 0;
     64            SYS_SubscribeToEvent(BC_EVENT_APS_DATA_INDICATION, &iasACEEventReceiver);
     65            SYS_SubscribeToEvent(BC_EVENT_NWK_DATA_CONFIRM, &iasACEEventReceiver);
     66          }
     67          
     68          /**************************************************************************//**
     69          \brief Initializes diagnostics cluster
     70          \param[in] eventId - eventId list for Diagnostic Cluster
     71          \param[in] data - data associated to Diagnostic CLuster Event Id
     72          ******************************************************************************/
     73          static void iasACEDiagnosticsEventListener(SYS_EventId_t eventId, SYS_EventData_t data)
     74          {
     75            switch(eventId)
     76            {
     77              case BC_EVENT_APS_DATA_INDICATION:
     78                if(NULL != (APS_DataQualityIndicator_t *) data)
     79                {
     80                  APS_DataQualityIndicator_t *apsDataQualityIndicator = (APS_DataQualityIndicator_t *)data;
     81                  iasACEDiagnosticsClusterServerAttributes.lastMessageLQI.value = apsDataQualityIndicator->linkQuality;
     82                  iasACEDiagnosticsClusterServerAttributes.lastMessageRSSI.value = apsDataQualityIndicator->rssi;
     83                }
     84                break;
     85          
     86              case BC_EVENT_NWK_DATA_CONFIRM:
     87                if(NULL != (uint16_t*) data)
     88                {
     89                  static bool firstPacket = 1;
     90                  if(firstPacket)
     91                  {
     92                    iasACEDiagnosticsClusterServerAttributes.averageMACRetryPerAPSMessageSent.value
     93                      = *(uint16_t*)data;
     94                    firstPacket = 0;
     95                  }
     96                  else
     97                  {
     98                    iasACEDiagnosticsClusterServerAttributes.averageMACRetryPerAPSMessageSent.value
     99                      = (iasACEDiagnosticsClusterServerAttributes.averageMACRetryPerAPSMessageSent.value 
    100                        + *(uint16_t*)data) / 2;
    101                  }
    102                }
    103                break;
    104              default:
    105                break;
    106            }
    107          }
