     36          /******************************************************************************
     37          \brief Calculates CRC of specified data.
     38          
     39          \param[in] initValue - CRC initial value.
     40          \param[in] data - pointer to data to be processed.
     41          \param[in] data - data length in bytes.
     42          
     43          \return Result of calculation.
     44          ******************************************************************************/
     45          PDS_FileCrc_t pdsCalculateRAMDataCrc(PDS_FileCrc_t initValue, uint8_t *data, uint16_t length)
     46          {
     47            PDS_FileCrc_t crc = initValue;
     48          
     49            for (uint16_t i = 0; i < length; i++)
     50              crc += data[i];
     51          
     52            return crc;
     53          }
     54          
     55          /******************************************************************************
     56          \brief Check if any valid data exists in non-volatile memory.
     57          
     58          \param[out] memoryMask - pointer to mask of PDTs to be checked on presence
     59                                      in non-volatile memory.
     60          
     61          \return Operation result, and mask of PDTs been found in non-volatile memory.
     62          ******************************************************************************/
     63          PDS_DataServerState_t pdsCheckStorage(PDS_MemMask_t memoryMask)
     64          {
     65            MEMORY_DESCRIPTOR fileDataDescr;
     66            PdsDataAccessContext_t accessContext = {{0u}, 0u};
     67          
     68            pdsPrepareAccessContext(&accessContext, memoryMask);
     69            PDS_MEM_MASK_CLEAR_ALL(memoryMask);
     70          
     71            while (pdsPrepareMemoryAccess(&accessContext, &fileDataDescr))
     72            {
     73              if (PDS_SUCCESS == pdsCheckFile(accessContext.memoryId, &fileDataDescr))
     74              {
     75                PDS_MEM_MASK_SET_BIT(memoryMask, accessContext.memoryId);
     76              }
     77            }
     78          
     79            return PDS_SUCCESS;
     80          }
     81          
     82          /******************************************************************************
     83          \brief Check file data within non-volatile storage.
     84          
     85          \param[in] memoryId - file unique identificator.
     86          \param[in] fileDataDescr - file payload memory access descriptor.
     87          
     88          \return result of checking.
     89          ******************************************************************************/
     90          PDS_DataServerState_t pdsCheckFile(PDS_MemId_t memoryId, const MEMORY_DESCRIPTOR *const fileDataDescr)
     91          {
     92            uint8_t data;
     93            PDS_FileHeader_t header;
     94            MEMORY_DESCRIPTOR accessDescriptor;
     95            PDS_FileCrc_t fileCrc = PDS_CRC_INITIAL_VALUE;
     96          
     97            accessDescriptor.data = (uint8_t *)&header;
     98            accessDescriptor.length = sizeof(PDS_FileHeader_t);
     99            accessDescriptor.address = fileDataDescr->address - sizeof(PDS_FileHeader_t);
    100            pdsRead(&accessDescriptor, pdsDummyCallback);
    101          
    102            if (memoryId != header.memoryId ||
    103                fileDataDescr->length != header.size)
    104              return PDS_CRC_ERROR;
    105          
    106            accessDescriptor.data = &data;
    107            accessDescriptor.length = 1;
    108          
    109            /* Check for any changes in byte-by-byte order */
    110            for (uint16_t i = 0; i < fileDataDescr->length; i++)
    111            {
    112              accessDescriptor.address = fileDataDescr->address + i;
    113              pdsRead(&accessDescriptor, pdsDummyCallback);
    114              fileCrc = pdsCalculateRAMDataCrc(fileCrc, accessDescriptor.data, 1);
    115            }
    116          
    117            if (fileCrc != header.crc)
    118              return PDS_CRC_ERROR;
    119          
    120            return PDS_SUCCESS;
    121          }
