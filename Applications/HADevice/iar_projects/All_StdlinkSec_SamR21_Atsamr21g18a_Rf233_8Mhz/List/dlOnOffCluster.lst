     55            dlOnOffClusterServerAttributes.onOff.value = false;
     56          }
     57          
     58          /**************************************************************************//**
     59          \brief Sets on/off state
     60          ******************************************************************************/
     61          void onOffSetOnOffState(bool state)
     62          {
     63            bool previousOnOffValue = dlOnOffClusterServerAttributes.onOff.value;
     64          
     65            dlOnOffClusterServerAttributes.onOff.value = state;
     66            ZCL_ReportOnChangeIfNeeded(&dlOnOffClusterServerAttributes.onOff);
     67            if (previousOnOffValue != state)
     68            {
     69              scenesInvalidate();
     70              PDS_Store(APP_DL_ONOFF_MEM_ID);
     71            }
     72          }
     73          
     74          /**************************************************************************//**
     75          \brief gets on/off state
     76          
     77          \return on/off state
     78          ******************************************************************************/
     79          bool onOffState(void)
     80          {
     81            return dlOnOffClusterServerAttributes.onOff.value;
     82          }
