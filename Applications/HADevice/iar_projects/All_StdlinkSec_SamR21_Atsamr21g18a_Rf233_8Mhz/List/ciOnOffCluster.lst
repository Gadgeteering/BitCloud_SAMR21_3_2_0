     55          /**************************************************************************//**
     56          \brief Sends On, Off or Toggle command to bound devices
     57          
     58          \param[in] mode    - address mode;
     59          \param[in] addr    - short address of destination node or number of group;
     60          \param[in] ep      - endpoint number of destination device;
     61          \param[in] command - command id
     62          ******************************************************************************/
     63          void onOffSendOnOffToggle(APS_AddrMode_t mode, ShortAddr_t addr, Endpoint_t ep,
     64            OnOffToggle_t command)
     65          {
     66            ZCL_Request_t *req;
     67          
     68            if (!(req = getFreeCommand()))
     69              return;
     70          
     71            fillCommandRequest(req, command, 0);
     72            fillDstAddressing(&req->dstAddressing, mode, addr, ep, ONOFF_CLUSTER_ID);
     73            commandManagerSendCommand(req);
     74          }
     75          /**************************************************************************//**
     76          \brief Sends the Configure Reporting for onOff cluster
     77          
     78          \param[in] mode - address mode;
     79          \param[in] addr - short address of destination node;
     80          \param[in] ep   - destination endpoint;
     81          \param[in] attrId - attribute id;
     82          \param[in] attrType - attribute type
     83          \param[in] min  - the minimum reporting interval;
     84          \param[in] max  - the maximum reporting interval
     85          ******************************************************************************/
     86          void onOffConfigureReporting(APS_AddrMode_t mode, ShortAddr_t addr, Endpoint_t ep,
     87            ZCL_AttributeId_t attrId, uint8_t attrType, ZCL_ReportTime_t min, ZCL_ReportTime_t max)
     88          {
     89            ZCL_Request_t *req;
     90            ZCL_NextElement_t element;
     91            ZCL_ConfigureReportingReq_t configureReportingReq;
     92          
     93            if (!(req = getFreeCommand()))
     94              return;
     95          
     96            configureReportingReq.direction            = ZCL_FRAME_CONTROL_DIRECTION_CLIENT_TO_SERVER;
     97            configureReportingReq.attributeId          = attrId;
     98            configureReportingReq.attributeType        = attrType;
     99            configureReportingReq.minReportingInterval = min;
    100            configureReportingReq.maxReportingInterval = max;
    101          
    102            element.payloadLength = 0;
    103            element.payload = req->requestPayload;
    104            element.id = ZCL_CONFIGURE_REPORTING_COMMAND_ID;
    105            element.content = &configureReportingReq;
    106            ZCL_PutNextElement(&element);
    107          
    108            fillCommandRequest(req, ZCL_CONFIGURE_REPORTING_COMMAND_ID, element.payloadLength);
    109            fillDstAddressing(&req->dstAddressing, mode, addr, ep, ONOFF_CLUSTER_ID);
    110            req->ZCL_Notify = ZCL_ConfigureReportingResp;
    111          
    112            commandManagerSendAttribute(req);
    113          }
