     46          /******************************************************************************
     47          \brief PDS timer initialization routine.
     48          ******************************************************************************/
     49          void pdsTimerInit(void)
     50          {
     51            pdsMemory()->periodicCommitTimer.mode = TIMER_REPEAT_MODE;
     52            pdsMemory()->periodicCommitTimer.callback = pdsTimerFired;
     53          }
     54          
     55          /***************************************************************************//**
     56          \brief Enables periodic commitment of specified persistent items to a
     57                 non-volatile storage.
     58          
     59          \ingroup pds
     60          
     61          \param[in] interval - period of time between successive commitments
     62          \param[in] memoryId - an identifier of PDS file or directory to be stored
     63                                in non-volatile memory.
     64          ******************************************************************************/
     65          void PDS_StoreByTimer(uint32_t interval, PDS_MemId_t memoryId)
     66          {
     67            HAL_StopAppTimer(&pdsMemory()->periodicCommitTimer);
     68          
     69            /* Disable periodic commitment, if zero passed as interval */
     70            if (!interval)
     71            {
     72              pdsMemory()->memIdToStoreByTimer = PDS_NO_MEMORY_SPECIFIED;
     73              return;
     74            }
     75          
     76            /* Reconfigure and start periodic commitment */
     77            pdsMemory()->periodicCommitTimer.interval = interval;
     78            pdsMemory()->memIdToStoreByTimer = memoryId;
     79            HAL_StartAppTimer(&pdsMemory()->periodicCommitTimer);
     80          }
     81          
     82          /******************************************************************************
     83          \brief PDS timer expiration callback.
     84          ******************************************************************************/
     85          static void pdsTimerFired(void)
     86          {
     87            PDS_Store(pdsMemory()->memIdToStoreByTimer);
     88          }
     89          
     90          /***************************************************************************//**
     91          \brief Stops the Persistent Data Server. This doesn't affect a policy created
     92                 through PDS_StoreByEvent() of  PDS_StoreByTimer() API.
     93                 After calling PDS_Run() function PDS continues it's work, as before the
     94                 stopping.
     95          *******************************************************************************/
     96          void PDS_Stop(void)
     97          {
     98            HAL_StopAppTimer(&pdsMemory()->periodicCommitTimer);
     99          
    100            while (pdsMemory()->status & PDS_WRITING_INPROGRESS_FLAG)
    101            {
    102              SYS_ForceRunTask();
    103            }
    104          
    105            pdsMemory()->status |= PDS_STOPPED_FLAG;
    106          }
    107          
    108          /***************************************************************************//**
    109          \brief Continue Persistent Data Server operation the same way, as before a
    110                 stopping.
    111          *******************************************************************************/
    112          void PDS_Run(void)
    113          {
    114            if (pdsMemory()->status & PDS_STOPPED_FLAG)
    115            {
    116              HAL_StartAppTimer(&pdsMemory()->periodicCommitTimer);
    117              pdsMemory()->status ^= PDS_STOPPED_FLAG;
    118            }
    119          }
