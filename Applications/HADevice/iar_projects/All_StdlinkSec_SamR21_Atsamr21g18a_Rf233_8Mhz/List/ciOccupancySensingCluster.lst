    194          /**************************************************************************//**
    195          \brief Indication of read attribute response
    196          
    197          \param[in] resp - pointer to response
    198          ******************************************************************************/
    199          static void ZCL_ReadAttributeResp(ZCL_Notify_t *ntfy)
    200          {
    201            ZCL_NextElement_t element;
    202            ZCL_ReadAttributeResp_t *readAttributeResp;
    203          
    204            if (ZCL_SUCCESS_STATUS == ntfy->status)
    205            {
    206              element.id            = ZCL_READ_ATTRIBUTES_RESPONSE_COMMAND_ID;
    207              element.payloadLength = ntfy->responseLength;
    208              element.payload       = ntfy->responsePayload;
    209              element.content       = NULL;
    210          
    211              ZCL_GetNextElement(&element);
    212              readAttributeResp = (ZCL_ReadAttributeResp_t *) element.content;
    213          
    214              if (readAttributeCallback)
    215                readAttributeCallback((void *)&readAttributeResp->value[0]);
    216          
    217              LOG_STRING(readAttrSuccessStr, " <-Read Occupancy Sensing attribute (0x%x) response: success t = %u\r\n");
